<nav class="speaker-page-navigation"
     data-component="speaker-page-navigation">
  <% if (pageNavigatioData.firstPage) { %>
    <div class="speaker-page-navigation__button"
         data-element="speaker-page-navigation__button">
      <a class="speaker-page-navigation__link"
         data-element="speaker-page-navigation__link"
         href="<%= paths.publicPathBackEnd %>/public-speakers?<%=category%>page=<%=pageNavigatioData.firstPage%>">
        <%= pageNavigatioData.firstPage %>
      </a>
    </div>
  <% } %>

  <% if (pageNavigatioData.dotsBefore) { %>
    <div class="speaker-page-navigation__dots"
         data-element="speaker-page-navigation__dots">
      <%='...'%>
    </div>
  <% } %>

  <% if (pageNavigatioData.pagesBefore[0]) { %>
    <div class="speaker-page-navigation__button"
         data-element="speaker-page-navigation__button">
      <a class="speaker-page-navigation__link"
         data-element="speaker-page-navigation__link"
         href="<%= paths.publicPathBackEnd %>/public-speakers?<%=category%>page=<%= pageNavigatioData.pagesBefore[0] %>">
        <%= pageNavigatioData.pagesBefore[0] %>
      </a>
    </div>
  <% } %>

  <% if (pageNavigatioData.pagesBefore[1]) { %>
    <div class="speaker-page-navigation__button"
         data-element="speaker-page-navigation__button">
      <a class="speaker-page-navigation__link"
         data-element="speaker-page-navigation__link"
         href="<%= paths.publicPathBackEnd %>/public-speakers?<%=category%>page=<%= pageNavigatioData.pagesBefore[1] %>">
        <%= pageNavigatioData.pagesBefore[1] %>
      </a>
    </div>
  <% } %>

  <% if (pageNavigatioData.currentPage) { %>
    <div class="speaker-page-navigation__button"
         data-element="speaker-page-navigation__button">
      <a class="speaker-page-navigation__link speaker-page-navigation__link_selected"
         data-element="speaker-page-navigation__link"
         href="#">
        <%= pageNavigatioData.currentPage %>
      </a>
    </div>
  <% } %>

  <% if (pageNavigatioData.pagesAfter[0]) { %>
    <div class="speaker-page-navigation__button"
         data-element="speaker-page-navigation__button">
      <a class="speaker-page-navigation__link"
         data-element="speaker-page-navigation__link"
         href="<%= paths.publicPathBackEnd %>/public-speakers?<%=category%>page=<%= pageNavigatioData.pagesAfter[0] %>">
        <%= pageNavigatioData.pagesAfter[0] %>
      </a>
    </div>
  <% } %>

  <% if (pageNavigatioData.pagesAfter[1]) { %>
    <div class="speaker-page-navigation__button"
         data-element="speaker-page-navigation__button">
      <a class="speaker-page-navigation__link"
         data-element="speaker-page-navigation__link"
         href="<%= paths.publicPathBackEnd %>/public-speakers?<%=category%>page=<%= pageNavigatioData.pagesAfter[1] %>">
        <%= pageNavigatioData.pagesAfter[1] %>
      </a>
    </div>
  <% } %>

  <% if (pageNavigatioData.dotsAfter) { %>
    <div class="speaker-page-navigation__dots"
         data-element="speaker-page-navigation__dots">
      <%='...'%>
    </div>
  <% } %>

  <% if (pageNavigatioData.lastPage) { %>
    <div class="speaker-page-navigation__button"
         data-element="speaker-page-navigation__button">
      <a class="speaker-page-navigation__link"
         data-element="speaker-page-navigation__link"
         href="<%= paths.publicPathBackEnd %>/public-speakers?<%=category%>page=<%= pageNavigatioData.lastPage %>">
        <%= pageNavigatioData.lastPage %>
      </a>
    </div>
  <% } %>
</nav>
